    <parameter lcioInType="SimCalorimeterHit" name="HCALCollections" type="StringVec">HCalBarrelCollection HCalEndcapCollection HCalRingCollection </parameter>
    
    <parameter lcioOutType="CalorimeterHit" name="HCALOutputCollection0" type="string">HCALBarrel </parameter>
    
